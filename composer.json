{
  "name": "thangnm93/coccoc-shipping-service",
  "type": "php-extension",
  "description": "It helps Vietnamese buy products on Amazon website.",
  "authors": [
    {
      "name": "Thang Nguyen",
      "email": "minhthang0403@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php" : ">=7.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5||^8.0"
  },
  "autoload": {
    "psr-4": {
      "Coccoc\\ShippingService\\": "src\\"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Coccoc\\ShippingService\\Tests\\": "tests\\"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --testdox --coverage-text --coverage-clover=coverage.clover"
  }
}